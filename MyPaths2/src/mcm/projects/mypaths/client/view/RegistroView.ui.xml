<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/GlobalStyles.css" />
	<g:HTMLPanel>
		<g:FormPanel>
			<g:DecoratorPanel>
				<g:VerticalPanel>
					<g:Label ui:field="tituloUsuario" width="120px"/>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="usuarioLabel" width="120px"/>
						<g:TextBox ui:field="usernameInput"/>				
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="passwordLabel" width="120px"/>
						<g:PasswordTextBox ui:field="passwordInput"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="rePasswordLabel" width="120px"/>
						<g:PasswordTextBox ui:field="rePasswordInput"/>
					</g:HorizontalPanel>
					<g:Label ui:field="tituloDatosPersonales" width="120px"/>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="emailLabel" width="120px"/>
						<g:TextBox ui:field="emailInput"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="nombreLabel" width="120px"/>
						<g:TextBox ui:field="nombreInput"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="apellidosLabel" width="120px"/>
						<g:TextBox ui:field="apellidosInput"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="paisLabel" width="120px"/>
						<g:ListBox ui:field="listaPaises"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Label ui:field="fotoLabel" width="120px"/>
						<g:FileUpload ui:field="fotoUpload"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:CheckBox ui:field="chkCondiciones"/>
						<g:Label ui:field="aceptoTxt" width="65px"/>
						<g:Hyperlink ui:field="aceptoLink" />
					</g:HorizontalPanel>
					<g:HorizontalPanel horizontalAlignment="right">
						<g:Button ui:field="registroButton" styleName="boton"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:DecoratorPanel>
		</g:FormPanel>
	</g:HTMLPanel>
</ui:UiBinder> 